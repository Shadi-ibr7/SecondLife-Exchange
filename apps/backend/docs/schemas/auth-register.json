{
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 10,
      "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]"
    },
    "displayName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 50
    }
  },
  "required": [
    "email",
    "password",
    "displayName"
  ],
  "additionalProperties": false
}